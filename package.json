{
  "name": "entandem",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-i18n:en": "ng build --prod --output-path dist/en --base-href /en/",
    "build-i18n:fr": "ng build --prod --i18n-file src/translate/messages.fr.xlf --i18n-format xlf --i18n-locale fr --output-path dist/fr --base-href /fr/",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:fr",
    "build-i18n-dev:en": "ng build --prod --configuration=dev --output-path dist/en --base-href /en/",
    "build-i18n-dev:fr": "ng build --prod --configuration=dev --i18n-file src/translate/messages.fr.xlf --i18n-format xlf --i18n-locale fr --output-path dist/fr --base-href /fr/",
    "build-i18n-dev": "npm run build-i18n-dev:en && npm run build-i18n-dev:fr",
    "build-i18n-test:en": "ng build --prod --configuration=test --output-path dist/en --base-href /en/",
    "build-i18n-test:fr": "ng build --prod --configuration=test --i18n-file src/translate/messages.fr.xlf --i18n-format xlf --i18n-locale fr --output-path dist/fr --base-href /fr/",
    "build-i18n-test": "npm run build-i18n-test:en && npm run build-i18n-test:fr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "packages": "0.0.8",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
